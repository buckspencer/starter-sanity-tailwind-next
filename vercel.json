{
  "version": 2,
  "builds": [
    {
      "src": "next/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/next/$1"
    }
  ],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": "next/.next",
  "rootDirectory": "next"
}
